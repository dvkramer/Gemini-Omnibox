{
  "manifest_version": 3,
  "name": "Gemini Omnibox Submitter",
  "version": "1.0",
  "description": "Submits omnibox input to Gemini.",
  "permissions": [
    "omnibox",
    "tabs",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": "gem"
  },
  "icons": {
    "128": "icon128.png"
  }
}
